{
  "name": "parrot-ui",
  "version": "1.0.0",
  "description": "UI Guidelines to Parrot UI",
  "author": "afvalenciab",
  "license": "MIT",
  "repository": "https://github.com/afvalenciab/parrot",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
